#include <iostream>
using namespace std;
int main() {
  int row, column;
  cout << "Enter The Toatal Row Of Rectangle" << endl;
  cin >> row;
  cout << "Enter The Toatal Coulum Of Rectangle" << endl;
  cin >> column;

  for (int i = 0; i < row; i++) {
    if (i == 0 || i == row - 1) {
      for (int j = 0; j < row; j++) {
        cout << "* ";
      }
    } else {
      cout << "* ";
      for (int m = 0; m < column - 2; m++) {
        cout << "  ";
      }
      cout << "* ";
    }

    cout << endl;
  }
}
